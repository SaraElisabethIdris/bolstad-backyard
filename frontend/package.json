    "@supabase/supabase-js": "^2.49.4",
    "@tanstack/react-table": "^8.20.5",
    "autoprefixer": "^10.4.20",
    "esbuild": "^0.25.0",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.2",
    "react-query": "^3.39.3",
    "react-router-dom": "6.26.2",
