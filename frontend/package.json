    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^4.3.1",
    "globals": "^16.0.0",
    "json-server": "^1.0.0-beta.3",
    "less": "^4.2.0",
    "less-loader": "^12.2.0",
    "tailwindcss": "^4.0.7",
    "typescript": "^5.2.2",
    "vite": "^6.1.1",
    "vite-plugin-svgr": "^4.3.0"
